pipeline{
    agent any
    tools{
        maven 'Maven 1'
    }
    stages{
        stage("SCM Checkout"){
            steps{
            git 'https://github.com/samben01/KarateWithJenkins.git'
            }
        }
        stage("Maven Build"){
            steps{
                sh "${mvnHome}/bin/mvn package"
            }
        }
    }
}
